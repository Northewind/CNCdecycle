;DETAL: TEST5 (DIS,UAO,RPT,DLY WITH E-PARAMS)

;E1=2
;E2=4
;E3=6
E1=2
E2=4
E3=6

(DIS,"PROVERKA E3")
(UAO,6)
(UAO, E3)

(DIS,"PROVERKA E1")
G90 (DIS, "2") G90
G90 (DIS, E1  ) G90

(DIS,"PROVERKA E2")
G90(DLY, 4)  G90
G90(DLY,E2)G90

(RPT,  E2)
    (DIS, "OUTSIDE CYCLE (E2 TIMES)")
    (RPT,E1)
        (DIS, "INSIDE CYCLE (E1 TIMES)")
    (ERP)
(ERP)


